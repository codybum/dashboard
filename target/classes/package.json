{
  "name": "cresco-dashboard",
  "version": "0.1.0",
  "description": "Development module for the Cresco Dashboard project",
  "main": "mock-server.js",
  "scripts": {
    "bs": "browser-sync start --proxy \"http://localhost:5000\" --files \"js/*.js\" \"*.js\" \"*.mustache\"",
    "mock-server": "nodemon -e js,mustache mock-server.js",
    "serve": "npm-run-all --parallel mock-server bs"
  },
  "keywords": [
    "Cresco",
    "HTML",
    "Bootstrap",
    "CSS",
    "CoreUI",
    "Dashboard",
    "Admin"
  ],
  "author": {
    "name": "Caylin Hickey",
    "url": "https://tsuyoi.org",
    "github": "https://github.com/tsuyoi",
    "twitter": "https://twitter.com/caylin_hickey"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "browser-sync": "^2.24.4",
    "nodemon": "^1.17.5",
    "npm-run-all": "^4.1.3"
  },
  "dependencies": {
    "express": "^4.16.3",
    "mustache": "^2.3.0",
    "mustache-express": "^1.2.6"
  },
  "engines": {
    "node": ">=6"
  },
  "browserslist": [
    "last 1 major version",
    ">= 1%",
    "Chrome >= 45",
    "Firefox >= 38",
    "Edge >= 12",
    "Explorer >= 10",
    "iOS >= 9",
    "Safari >= 9",
    "Android >= 4.4",
    "Opera >= 30"
  ]
}
